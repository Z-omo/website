{
    "extends": ["eslint:recommended"],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "brace-style": 0,
        "valid-jsdoc": 0,
        "require-jsdoc": 0,
        "comma-dangle": 0,
        "key-spacing": [1, {
            "beforeColon": false,
            "afterColon": true,
            "mode": "minimum"
        }],
        "eol-last": 2
    }
}